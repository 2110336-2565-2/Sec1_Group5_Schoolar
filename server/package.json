	"dependencies": {
		"axios": "^1.3.6",
		"bcrypt": "^5.1.0",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-jwt": "^8.3.0",
		"express-validator": "^6.14.3",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^6.8.4",
		"morgan": "^1.10.0",
		"nodemailer": "^6.9.1",
